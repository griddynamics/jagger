<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:custom="urn:import:com.griddynamics.jagger.webclient.client.components">
    <ui:with type="com.griddynamics.jagger.webclient.client.resources.JaggerResources" field="res"/>
    <g:ScrollPanel ui:field="scrollPanel">
        <g:VerticalPanel width="100%" height="100%">
            <g:HorizontalPanel ui:field="buttonPanel" height="50" />
            <custom:DynamicLayoutPanel ui:field="pane" height="100%" width="100%"/>
        </g:VerticalPanel>
    </g:ScrollPanel>
</ui:UiBinder>
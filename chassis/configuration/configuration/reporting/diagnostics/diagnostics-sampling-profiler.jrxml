<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="LatencyReport" pageWidth="595" pageHeight="842" orientation="Landscape" columnWidth="550"
              leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
    <property name="ireport.zoom" value="1.0"/>
    <property name="ireport.x" value="0"/>
    <property name="ireport.y" value="0"/>
    <template class="java.io.InputStream">
        <![CDATA[$P{context}.getResource("/reporting/styles/default-styles.xml")]]></template>
    <parameter name="context" class="com.griddynamics.jagger.reporting.ReportingContext"/>
    <field name="sysUnderTestUrl" class="java.lang.String"/>
    <field name="methodId" class="java.lang.String"/>
    <field name="onTopRatio" class="java.lang.Double"/>
    <field name="inStackRatio" class="java.lang.Double"/>
    <field name="invocationGraphImage" class="java.awt.Image"/>
    <field name="traceback" class="java.util.List"/>
    <title>
        <band height="90" splitType="Prevent">
            <rectangle>
                <reportElement x="0" y="20" width="550" height="20" backcolor="#AAE600"/>
                <graphicElement>
                    <pen lineWidth="0.0" lineStyle="Solid"/>
                </graphicElement>
            </rectangle>
            <textField>
                <reportElement style="Header_Bold" positionType="Float" x="0" y="20" width="550" height="20"
                               forecolor="#336600"/>
                <textElement textAlignment="Center" verticalAlignment="Middle"/>
                <textFieldExpression><![CDATA["Sampling Profiler : from " + $F{sysUnderTestUrl}]]></textFieldExpression>
            </textField>
            <staticText>
                <reportElement style="Header_Bold" x="0" y="75" width="550" height="10"/>
                <textElement textAlignment="Left"/>
                <text><![CDATA[Hot Spots:]]></text>
            </staticText>
        </band>
    </title>
    <columnHeader>
        <band height="23" splitType="Stretch">
            <staticText>
                <reportElement style="Header_Bold" mode="Opaque" x="0" y="0" width="370" height="21" forecolor="#000000"
                               backcolor="#D7FFB0"/>
                <textElement textAlignment="Center" verticalAlignment="Middle"/>
                <text><![CDATA[Method]]></text>
            </staticText>
            <staticText>
                <reportElement style="Header_Bold" mode="Opaque" x="370" y="0" width="90" height="21"
                               forecolor="#000000" backcolor="#D7FFB0"/>
                <textElement textAlignment="Center" verticalAlignment="Middle"/>
                <text><![CDATA[Self Time Ratio]]></text>
            </staticText>
            <staticText>
                <reportElement style="Header_Bold" mode="Opaque" x="460" y="0" width="90" height="21"
                               forecolor="#000000" backcolor="#D7FFB0"/>
                <textElement textAlignment="Center" verticalAlignment="Middle"/>
                <text><![CDATA[In Stack Ratio]]></text>
            </staticText>
        </band>
    </columnHeader>
    <detail>
        <band height="30" splitType="Stretch">
            <textField isStretchWithOverflow="true">
                <reportElement style="Header_Bold" x="0" y="0" width="370" height="10" forecolor="#FF0000"/>
                <textElement textAlignment="Left"/>
                <textFieldExpression><![CDATA[$F{methodId}]]></textFieldExpression>
            </textField>
            <textField pattern="##0.00%">
                <reportElement style="Header_Bold" x="370" y="0" width="90" height="10" forecolor="#FF0000"/>
                <textElement textAlignment="Center"/>
                <textFieldExpression class="java.lang.Double"><![CDATA[$F{onTopRatio}]]></textFieldExpression>
            </textField>
            <textField pattern="##0.00%">
                <reportElement style="Header_Bold" x="460" y="0" width="90" height="10" forecolor="#FF0000"/>
                <textElement textAlignment="Center"/>
                <textFieldExpression class="java.lang.Double"><![CDATA[$F{inStackRatio}]]></textFieldExpression>
            </textField>
            <subreport>
                <reportElement positionType="Float" x="20" y="12" width="530" height="10"/>
                <parametersMapExpression><![CDATA[$P{REPORT_PARAMETERS_MAP}]]></parametersMapExpression>
                <dataSourceExpression>
                    <![CDATA[new net.sf.jasperreports.engine.data.JRBeanCollectionDataSource($F{traceback})]]></dataSourceExpression>
                <subreportExpression class="net.sf.jasperreports.engine.JasperReport">
                    <![CDATA[$P{context}.getReport("/reporting/diagnostics/diagnostics-sampling-profiler-traceback.jrxml")]]></subreportExpression>
            </subreport>
        </band>
    </detail>
</jasperReport>

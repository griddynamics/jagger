<beans
        xmlns:jgr="http://www.griddynamics.com/schema/jagger"
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.griddynamics.com/schema/jagger
        http://www.griddynamics.com/schema/jagger.xsd
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
        >


    <jgr:test-plan id="tp1">
        <jgr:test>
            <jgr:task sample="34250" delay="0" bean="getTopLevelCategoryTreeCatalogService">
                <jgr:user count="50" startCount="50" startIn="0" startBy="0" life="2h"/>
            </jgr:task>
            <jgr:task sample="34250" delay="0" bean="getTopLevelCategoryTreeCatalogService">
                <jgr:invocation exactcount="10" threads="2"/>
            </jgr:task>
        </jgr:test>
        <ref local="tst1"/>
    </jgr:test-plan>

    <jgr:processing id="tp2">
        <jgr:test>
            <jgr:task sample="34250" delay="0" bean="getTopLevelCategoryTreeCatalogService">
                <jgr:user count="50" startCount="50" startIn="0" startBy="0" life="2h"/>
            </jgr:task>
            <jgr:task sample="34250" delay="0" bean="getTopLevelCategoryTreeCatalogService">
                <jgr:invocation exactcount="10" threads="2"/>
            </jgr:task>
        </jgr:test>
        <ref local="tst1"/>
    </jgr:processing>

    <jgr:task id="t1" name="task1" sample="34250" delay="0" bean="bean1" duration="200">
        <jgr:user id="u2" count="50" startCount="50" startIn="0" startBy="0" life="2h"/>
        <jgr:user id="u3" count="50" startCount="50" startIn="0" startBy="0" life="2h"/>
        <jgr:user id="u4" count="50" startCount="50" startIn="0" startBy="0" life="2h"/>
    </jgr:task>

    <jgr:test id="tst1" duration="2h" name="[catalog-service]-getCategoryTree-[top-level]-hessian">
        <jgr:task sample="34250" delay="0" bean="getTopLevelCategoryTreeCatalogService">
            <jgr:invocation exactcount="5" threads="2"/>
        </jgr:task>
        <ref local="t1"/>
    </jgr:test>

    <jgr:task id="t2" name="task2" sample="24250" delay="10" bean="bean2" duration="300">
        <jgr:invocation exactcount="5" threads="2"/>
    </jgr:task>

    <jgr:user id="u1" count="50" startCount="50" startIn="0" startBy="0" life="2h"/>

    <jgr:invocation id="i1" exactcount="5" threads="2"/>

    <bean name="bean1" class="com.griddynamics.jagger.user.ProcessingConfig$Test" lazy-init="false"></bean>
</beans>

<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
        xmlns="http://www.griddynamics.com/schema/jagger"
        xmlns:beans="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation=
                "
        http://www.griddynamics.com/schema/jagger
        http://www.griddynamics.com/schema/jagger.xsd
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
        >


    <beans:import resource="test.description.conf.xml"/>

    <!--Name of workload configuration-->
    <configuration id="JaaS_TestConfiguration">
        <latency-percentiles>
            <percentile>99</percentile>
        </latency-percentiles>

        <test-suite id="ts_JaaSTestSuit">
            <test-group id="tg_JaaS_GET_Sessions">
                <test id="t_JaaS_GET_Sessions_List_SR_eq._1.0" testDescription="td_JaaS_SessionsList">
                    <load xsi:type="load-user-groups">
                        <user count="2" startCount="2" startIn="0" startBy="0" life="${jaas.std.tst.life}" id="tl_JaaS_sessions" />
                    </load>
                    <termination xsi:type="termination-iterations" iterations="${jaas.std.tst.iterations}" maxDuration="${jaas.std.tst.max_duration}"/>
                    <limits xsi:type="limits-ref" ref="test_limits"/>
                </test>

                <test id="t_JaaS_GET_Exact_Session_SR_eq._1.0" testDescription="td_JaaS_ExactSessionIds">
                    <load xsi:type="load-user-groups">
                        <user count="2" startCount="2" startIn="0" startBy="0" life="${jaas.std.tst.life}" id="tl_JaaS_session_ids" />
                    </load>
                    <termination xsi:type="termination-iterations" iterations="${jaas.std.tst.iterations}" maxDuration="${jaas.std.tst.max_duration}"/>
                    <limits xsi:type="limits-ref" ref="test_limits"/>
                </test>

                <test id="t_JaaS_GET_Session_Tests_List_SR_eq._1.0" testDescription="td_JaaS_SessionTestsList">
                    <load xsi:type="load-user-groups">
                        <user count="2" startCount="2" startIn="0" startBy="0" life="${jaas.std.tst.life}" id="tl_JaaS_session_tests" />
                    </load>
                    <termination xsi:type="termination-iterations" iterations="${jaas.std.tst.iterations}" maxDuration="${jaas.std.tst.max_duration}"/>
                    <limits xsi:type="limits-ref" ref="test_limits"/>
                </test>

                <test id="t_JaaS_GET_Exact_Session_Test_SR_eq._1.0" testDescription="td_JaaS_ExactSessionTest">
                    <load xsi:type="load-user-groups">
                        <user count="2" startCount="2" startIn="0" startBy="0" life="${jaas.std.tst.life}" id="tl_JaaS_session_test" />
                    </load>
                    <termination xsi:type="termination-iterations" iterations="${jaas.std.tst.iterations}" maxDuration="${jaas.std.tst.max_duration}"/>
                    <limits xsi:type="limits-ref" ref="test_limits"/>
                </test>
            </test-group>

            <listeners-test-suite>
                <listener-test-suite xsi:type="listener-test-suite-ref" ref="listenerTestSuite"/>
            </listeners-test-suite>
        </test-suite>

        <report outputReportLocation="jaas-test-report.pdf" />

    </configuration>
</beans:beans>
